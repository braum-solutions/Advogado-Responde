<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.braumsolutions.advogadoresponde">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.LawyerCodeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".View.TypeRegistrationActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".View.NameRegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.ConfirmEmailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.PrivacyPoliceActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".View.NameRegisterActivity" />
        </activity>
        <activity
            android:name=".View.SignInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>